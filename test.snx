map results {
    type: .array;
    key: u32;
    value: u64;
    max: 8;
}

unit sample_tracepoint {
    section: "tracepoint/syscalls/sys_enter_execve";
    license: "GPL";

    reg a = 10;
    reg b = 20;

    reg c = a + b;   

    imm k0 = 0; heap p0 = results.lookup(k0); *p0 = c;

    return 0;
}
